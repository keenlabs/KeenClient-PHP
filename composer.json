{
    "name": "keen-io/keen-io",
    "description": "A PHP library for reporting events to the Keen IO API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "keen io",
        "analytics"
    ],
    "support": {
        "chat": "https://www.hipchat.com/gIdidQscL"
    },
    "require": {
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "guzzlehttp/guzzle-services": "^1.0",
        "php": ">=5.6.0",
        "paragonie/random_compat": "^1.4 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27 || ^8.5.32 || ^9.6.3",
        "squizlabs/php_codesniffer": "^3.5",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "homepage": "http://keen.io",
    "autoload": {
        "psr-4": {
            "KeenIO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeenIO\\Tests\\": "tests/Tests"
        }
    },
    "authors": [
        {
            "name": "Tom Anderson",
            "email": "tom.h.anderson@gmail.com"
        },
        {
            "name": "Keith Kirk",
            "email": "keith@kmfk.io",
            "homepage": "http://kmfk.io"
        },
        {
            "name": "MichaÃ«l Gallego",
            "email": "mic.gallego@gmail.com",
            "homepage": "http://michaelgallego.fr"
        }
    ],
    "scripts": {
        "phpcs": [
            "./vendor/bin/phpcs --colors --standard=PSR2 src"
        ],
        "phpunit": [
            "./vendor/bin/phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
